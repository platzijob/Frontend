    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__test__/setupTest.js"
